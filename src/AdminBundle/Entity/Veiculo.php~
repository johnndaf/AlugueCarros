<?php

namespace AdminBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * @ORM\Entity
 * @ORM\Table(name="veiculos")
 */
class Veiculo 
{
    /**
     * @ORM\Column(type="integer")
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="AUTO")
     */
    private $id;
    
    /**
     * @ORM\Column(type="string", length=40)
     */
    private $modelo;
    
    /**
     * @ORM\Column(type="string", length=40)
     */
    private $marca;
    
    /**
     * @ORM\Column(type="date")
     */
    private $ano;
    
    /**
     * @ORM\Column(type="string", length=40)
     */
    private $cor;
    
    /**
     * @ORM\Column(type="string", length=40)
     */
    private $categoria;
}
